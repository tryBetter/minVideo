<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="android:Theme.Holo.Light.NoActionBar">
        <!-- Customize your theme here. -->
    </style>

    <style name="MediaButtonCompat">
        <item name="android:background">@null</item>
        <item name="android:layout_width">71dip</item>
        <item name="android:layout_height">52dip</item>
    </style>

    <style name="MediaButtonCompat.Previous">
        <item name="android:src">@mipmap/vvc_ic_media_previous</item>
    </style>

    <style name="MediaButtonCompat.Next">
        <item name="android:src">@mipmap/vvc_ic_media_next</item>
    </style>

    <style name="MediaButtonCompat.Play">
        <item name="android:src">@mipmap/vvc_ic_media_play</item>
    </style>

    <style name="MediaButtonCompat.Ffwd">
        <item name="android:src">@mipmap/vvc_ic_media_ff</item>
    </style>

    <style name="MediaButtonCompat.Rew">
        <item name="android:src">@mipmap/vvc_ic_media_rew</item>
    </style>

    <style name="MediaButtonCompat.Pause">
        <item name="android:src">@mipmap/vvc_ic_media_pause</item>
    </style>


    <!--播放器进度条的styles-->
    <style name="SeekBarAppTheme">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:focusable">true</item>
        <item name="android:progressDrawable">
            @drawable/jky_player_progressbar
        </item>
        <item name="android:thumb">@drawable/ic_circles</item>
        <item name="android:minHeight">5dip</item>
        <item name="android:maxHeight">5dip</item>
        <item name="android:paddingLeft">5.0dip</item>
        <item name="android:paddingRight">5.0dip</item>
    </style>
    <!-- 播放器进度条end-->
</resources>
